{
    "name": "merchant-machine",
    "productName": "merchant-machine",
    "version": "1.0.0",
    "description": "Merchant machine",
    "main": "src/main/index.js",
    "scripts": {
        "analyze": "source-map-explorer build/static/js/main.*",
        "build-react": "react-scripts build",
        "start": "concurrently \"react-scripts start\" \"NODE_ENV=development electron-forge start\"",
        "package": "yarn build-react && NODE_ENV=production electron-forge package",
        "make": "yarn build-react && NODE_ENV=production electron-forge make",
        "lint": "eslint src --color"
    },
    "keywords": [],
    "author": "DHL",
    "license": "MIT",
    "homepage": "./",
    "config": {
        "forge": {
            "make_targets": {
                "win32": [
                    "squirrel"
                ],
                "darwin": [
                    "zip"
                ],
                "linux": [
                    "deb",
                    "rpm"
                ]
            },
            "electronPackagerConfig": {
                "packageManager": "yarn"
            },
            "electronWinstallerConfig": {
                "name": "merchant_machine"
            },
            "electronInstallerDebian": {},
            "electronInstallerRedhat": {},
            "github_repository": {
                "owner": "",
                "name": ""
            },
            "windowsStoreConfig": {
                "packageName": "",
                "name": "merchantmachine"
            }
        }
    },
    "dependencies": {
        "electron-compile": "^6.4.4",
        "electron-squirrel-startup": "^1.0.0",
        "react": "^16.8.4",
        "react-dom": "^16.8.4",
        "react-scripts": "2.1.8"
    },
    "devDependencies": {
        "babel-plugin-transform-async-to-generator": "^6.24.1",
        "babel-preset-env": "^1.7.0",
        "babel-preset-react": "^6.24.1",
        "concurrently": "^4.1.0",
        "electron": "^4.0.6",
        "electron-forge": "^5.2.4",
        "electron-prebuilt-compile": "4.0.0",
        "eslint-config-airbnb": "^15",
        "eslint-plugin-import": "^2",
        "eslint-plugin-jsx-a11y": "^5",
        "eslint-plugin-react": "^7",
        "node-sass": "^4.11.0",
        "npm-run-all": "^4.1.5",
        "source-map-explorer": "^1.7.0"
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie <= 11",
        "not op_mini all"
    ]
}