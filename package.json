{
    "name": "merchant-machine",
    "productName": "merchant-machine",
    "version": "1.0.1",
    "description": "Merchant machine",
    "main": "src/main/main.js",
    "scripts": {
        "analyze": "source-map-explorer build/static/js/main.*",
        "lint": "eslint src --color",
        "build-react": "react-app-rewired build",
        "prevbuild-react": "rm -rf build && rm-rf dist",
        "start-react": "react-app-rewired start",
        "start": "concurrently \"BROWSER=none yarn start-react\" \"wait-on http://localhost:3000 && electron .\"",
        "dist": "yarn build-react && electron-builder -c.extraMetadata.main=src/main/main.js",
        "dist-appimage": "yarn build-react && electron-builder -c.extraMetadata.main=src/main/main.js --linux appImage",
        "dist-nsis": "yarn build-react && electron-builder -c.extraMetadata.main=src/main/main.js --win --ia32 nsis",
        "release": "GH_TOKEN=eed28f94a4c8a8d197d49b28af6951c3c253022a build -c.extraMetadata.main=src/main/main.js"
    },
    "keywords": [],
    "author": "DHL",
    "license": "MIT",
    "homepage": "./",
    "repository": {
        "type": "git",
        "url": "https://github.com/dhl1402/merchant-machine.git"
    },
    "build": {
        "appId": "com.appota.merchant",
        "files": [
            "build/**/*",
            "node_modules/**/*",
            "src/main/main.js",
            "src/constants/**/*"
        ],
        "directories": {
            "buildResources": "assets"
        },
        "electronVersion": "4.0.0",
        "appImage": {
            "artifactName": "ACheckin Linux",
            "systemIntegration": "doNotAsk",
            "publish": {
                "provider": "github",
                "private": true
            }
        },
        "mac": {
            "target": "dmg",
            "publish": {
                "provider": "github",
                "private": true
            }
        },
        "nsis": {
            "artifactName": "ACheckin.${ext}",
            "createDesktopShortcut": "always",
            "publish": {
                "provider": "github",
                "private": true
            }
        },
        "compression": "store"
    },
    "dependencies": {
        "apollo-cache-inmemory": "^1.5.1",
        "apollo-client": "^2.5.1",
        "apollo-link": "^1.2.9",
        "apollo-link-context": "^1.0.15",
        "apollo-link-http": "^1.5.12",
        "apollo-link-timeout": "^1.2.1",
        "bootstrap-scss": "^4.3.1",
        "crypto": "^1.0.1",
        "electron-compile": "^6.4.4",
        "electron-is-dev": "^1.0.1",
        "electron-log": "^3.0.3",
        "electron-squirrel-startup": "^1.0.0",
        "electron-updater": "^4.0.6",
        "element-resize-event": "^3.0.3",
        "graphql": "^14.1.1",
        "graphql-tag": "^2.10.1",
        "immutability-helper": "^3.0.0",
        "lodash.debounce": "^4.0.8",
        "lodash.get": "^4.4.2",
        "moment": "^2.24.0",
        "node-machine-id": "^1.1.10",
        "node-schedule": "^1.3.2",
        "prop-types": "^15.7.2",
        "qrcode.react": "^0.9.3",
        "react": "^16.8.4",
        "react-apollo": "https://github.com/dhl1402/react-apollo.git#hooks",
        "react-bootstrap": "^1.0.0-beta.5",
        "react-custom-scrollbars": "^4.2.1",
        "react-dom": "^16.8.4",
        "react-dragscroll": "^2.0.1",
        "react-icons": "^3.5.0",
        "react-scripts": "2.1.8",
        "react-spinners": "^0.5.3",
        "socket.io-client": "^2.2.0",
        "yup": "^0.26.10"
    },
    "devDependencies": {
        "babel-plugin-transform-async-to-generator": "^6.24.1",
        "babel-preset-env": "^1.7.0",
        "babel-preset-react": "^6.24.1",
        "concurrently": "^4.1.0",
        "customize-cra": "^0.2.12",
        "electron": "^4.1.1",
        "electron-builder": "^20.39.0",
        "electron-prebuilt-compile": "4.0.0",
        "eslint-config-airbnb": "^15",
        "eslint-import-resolver-alias": "^1.1.2",
        "eslint-plugin-import": "^2",
        "eslint-plugin-jsx-a11y": "^5",
        "eslint-plugin-react": "^7",
        "node-sass": "^4.11.0",
        "npm-run-all": "^4.1.5",
        "react-app-rewired": "^2.1.1",
        "source-map-explorer": "^1.7.0",
        "wait-on": "^3.2.0"
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie <= 11",
        "not op_mini all"
    ]
}